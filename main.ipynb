    "def add_borders(l):\n",
    "    new_list = []\n",
    "    borders = '*' * (len(l) + 2)\n",
    "    new_list.(borders)\n",
    "    for s in l:\n",
    "        new_list.append('*' + s + '*')\n",
    "    new_list.append(borders)\n",
    "    return new_list\n",
    "\n",
    "\n",
    "borders = add_borders(['abc', 'dec', 'def'])\n",
    "for s in borders:\n",
    "    print(s)\n"
